<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="TfsServerUrl" value="http://dsaocatfs01:8080/tfs"/>
    <add key="TfsCollectionName" value="OCEANCollection"/>
    <add key="TfsProjectName" value="OCEAN"/>
    <add key="SharePointWebUrl" value="http://ododsharepoint/sites/ocs_development/"/>
    <add key="SharepointCalendarName" value="Calendar"/>
    <add key="HolidayFile" value="Holidays.xml"/>
    <add key="NumberOfWeeks" value="4"/>
    <add key="EstimatedMaxCheckinsPerWeek" value="50"/> <!-- Increasing this will decrease TFS query performance but may be necessary if you have a ton of checkins-->
    <add key="StartTimeSheetDataWebService" value="false"/>
    <add key="TimeSheetDataWebServiceUrl" value="http://localhost:40347/timesheet/"/>
  </appSettings>
<startup>
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
</startup>
</configuration>
